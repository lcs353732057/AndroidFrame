<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.liaochunsheng.androiddemo">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GlideActivity" />
        <activity android:name=".RxJavaActivity" />
        <activity android:name=".OkHttpActivity" />
        <activity android:name=".RecyclerViewActivity" />
        <activity android:name=".PicassoActivity" />
        <activity android:name=".PatchDexActivity" />
        <activity android:name=".EventActivity" />
        <activity android:name=".HandlerActivity" />
        <activity android:name=".TouchImageViewActivity" />
        <activity android:name=".ToolbarActivity" />
        <activity android:name=".VitamioActivity" />
        <activity android:name=".ImageViewRecoveryActivity"></activity>
    </application>

</manifest>